<table class="table table-striped">
    <thead>
    <tr>
        <th>Date</th>
        <th>Description</th>
        <th>Amount</th>
        <th>Category</th>
    </tr>
    </thead>
    <tbody>
    {{#rows}}
    <tr>
        <td>{{date}}</td>
        <td>{{description}}</td>
        <td>{{amount}}</td>
        <td>{{category}}</td>
    </tr>
    {{/rows}}
    </tbody>
</table>